CREATE TABLE IF NOT EXISTS `RecordingSchedules` (
	`ID`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`Schedule`	INTEGER,
	`Channel`	TEXT,
	`StartTime`	TEXT,
	`EndTime`	TEXT,
	`Monday`	INTEGER,
	`Tuesday`	INTEGER,
	`Wednesday`	INTEGER,
	`Thursday`	INTEGER,
	`Friday`	INTEGER,
	`Saturday`	INTEGER,
	`Sunday`	INTEGER,
	`Repeat`	INTEGER
);
CREATE TABLE IF NOT EXISTS `TVGuide` (
	`Channel`	INTEGER,
	`Title`	TEXT,
	`Description`	TEXT,
	`StartTime`	TEXT,
	`Date`	TEXT,
	`Time`	TEXT,
	`Day`	TEXT,
	`Duration`	INTEGER,
	`Properties`	TEXT
);